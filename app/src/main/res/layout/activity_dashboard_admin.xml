<?xml version="1.0" encoding="utf-8"?>
<androidx.coordinatorlayout.widget.CoordinatorLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".ui.dashboard.DashboardAdminActivity">

    <com.google.android.material.bottomnavigation.BottomNavigationView
            android:id="@+id/dashboard_navigation_view"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="bottom"
            android:background="@android:color/white"
            android:elevation="@dimen/elevation_high"
            android:theme="@style/Voucherify.Base.Widget.BottomNavigationView"
            app:elevation="@dimen/elevation_high"
            app:itemBackground="@color/voucherify_background"
            app:itemIconTint="@drawable/bottom_navigation_item_selector"
            app:itemTextColor="@drawable/bottom_navigation_item_selector"
            app:layout_insetEdge="bottom"
            app:menu="@menu/dashboard_admin"/>

</androidx.coordinatorlayout.widget.CoordinatorLayout>